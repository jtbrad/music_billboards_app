<h1>Artists</h1>

<hr />

<div>
  <%= link_to "Return To Billboards", billboards_path, class: "ui button" %>
  <%= link_to "Add New Artist", new_artist_path, class: "ui button" %>
</div>

<div class="ui list">
  <% @artists.each do |artist| %>
    <div class="item">
      <%= link_to "#{artist.full_name}", artist_path(artist) %>
    </div>
  <% end %>
</div>